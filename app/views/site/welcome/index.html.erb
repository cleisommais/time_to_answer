<h1>Site::Welcome#index</h1>
<p>Find me in app/views/site/welcome/index.html.erb</p>

<% if user_signed_in?  %>
  <p><%= t :user_signed %> | <strong><%= current_user.email %></strong></p>
<% else  %>  
  <p><%= t :user_logout %> </p>
<% end  %>

<% if admin_signed_in?  %>
  <p><%= t :admin_signed %> | <strong><%= current_admin.email %></strong></p>
<% else  %>  
  <p><%= t :admin_logout %> </p>
<% end  %>
